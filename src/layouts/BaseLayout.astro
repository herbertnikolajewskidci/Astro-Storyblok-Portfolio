---
import CustomStyles from "../components/CustomStyles.astro";
const { title } = Astro.props;
---

<html lang="de">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <link rel="icon" type="image/x-icon" href="/favicon.ico" />
        <CustomStyles />
        <title>{title}</title>
    </head>
    <body class="bg-background">
        <slot />
    </body>
</html>
